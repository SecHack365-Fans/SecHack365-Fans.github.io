{
  "name": "sechack365_fans",
  "private": true,
  "engines": {
    "node": ">=18.12.0"
  },
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "format": "prettier --write \"(pages|components|styles|lib)/**/*.{js,jsx,ts,tsx,json,md,css}\"",
    "all-format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
